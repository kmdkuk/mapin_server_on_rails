<%= form_for([@company, @shop, @file], url: company_shop_files_path(@company,@shop)) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: "デフォルトでファイル名になります。", class: 'form-control' %>
    
    <%= f.label :file %>
    <%= f.file_field :file, class: 'form-control' %>

    <%= f.submit "submit", class: "btn btn-primary" %>
<% end %>
