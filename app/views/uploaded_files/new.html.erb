<%= form_for([@company, @shop, @file], url: company_shop_files_path(@company,@shop)) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.label :file_type %>
    <%= f.text_field :file_type, class: 'form-control' %>

    <%= f.label :file %>
    <%= f.file_field :url, class: 'form-control' %>

    <%= f.submit "submit", class: "btn btn-primary" %>
<% end %>
