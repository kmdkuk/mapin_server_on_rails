<%= form_for([@company, @shop, @file], url: new_company_shop_file_path) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.label :file_type %>
    <%= f.text_field :file_type, class: 'form-control' %>

    <%= f.label :url %>
    <%= f.text_field :url, class: 'form-control' %>

    <%= f.submit "submit", class: "btn btn-primary" %>
<% end %>
